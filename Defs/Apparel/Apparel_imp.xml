<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>


<ThingDef Name="ImpApparelBase" Abstract="True">
		<thingClass>Apparel</thingClass>
		<category>Item</category>
		<selectable>True</selectable>
		<pathCost>10</pathCost>
		<useHitPoints>True</useHitPoints>
		<graphicData>
			<onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
		</graphicData>
		<drawGUIOverlay>true</drawGUIOverlay>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>0.5</Flammability>
			<DeteriorationRate>0.5</DeteriorationRate>
			<SellPriceFactor>0.75</SellPriceFactor>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<tickerType>Never</tickerType>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li>
				<compClass>CompColorable</compClass>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
	</ThingDef>


	<ThingDef Name="ImpApparelMakeableBase" ParentName="ImpApparelBase" Abstract="True">
		<recipeMaker>
			<workSpeedStat>TailoringSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
	</ThingDef>
	
	<ThingDef Name="ImpArmorMakeableBase" ParentName="ImpApparelBase" Abstract="True">
		<recipeMaker>
			<workSpeedStat>SmithingSpeed</workSpeedStat>
			<workSkill>Crafting</workSkill>
			<effectWorking>Smith</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<recipeUsers>
				<li>TableMachining</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
		</recipeMaker>
	</ThingDef>
	
	<ThingDef Name="ImpHatBase" ParentName="ImpApparelBase" Abstract="True">
		<thingCategories>
			<li>Headgear</li>
		</thingCategories>
	</ThingDef>

	<ThingDef Name="ImpHatMakeableBase" ParentName="ImpApparelMakeableBase" Abstract="True">
		<thingCategories>
			<li>Headgear</li>
		</thingCategories>
	</ThingDef>
	


	<ThingDef Name="ImpHelmetMakeableBase" ParentName="ImpArmorMakeableBase" Abstract="True">
		<thingCategories>
			<li>Headgear</li>
		</thingCategories>
	</ThingDef>



  <ThingDef ParentName="ImpApparelMakeableBase">
    <defName>PJ_ImpUniform</defName>
    <label>Imperial Uniform</label>
    <techLevel>Spacer</techLevel>
    <description>The uniform worn by the martial forces of the Galactic Empire.</description>
    <graphicData> 
		<texPath>Imp/Gear/ImperialOfficerUniform</texPath>
		<graphicClass>Graphic_Single</graphicClass> 
	</graphicData>
    <statBases>
      <Mass>1</Mass>
			<MaxHitPoints>350</MaxHitPoints>
			<MarketValue>100</MarketValue>
			<ArmorRating_Blunt>0.3</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.3</ArmorRating_Sharp>
			<ArmorRating_Heat>0.3</ArmorRating_Heat>
			<Insulation_Cold>-15</Insulation_Cold>
			<Insulation_Heat>5</Insulation_Heat>
    </statBases>
	<costList>
      <Steel>8</Steel>
      <Cloth>120</Cloth>
    </costList>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <apparel>
     <tags>
       <li>PJ_Imp</li>
     </tags>
     <defaultOutfitTags>
        <li>Worker</li>
        <li>Soldier</li>
     </defaultOutfitTags>
     <worngraphicPath>Imp/Gear/ImperialOfficerUniform</worngraphicPath>
			<layers>
			    <li>OnSkin</li>
			</layers>
     <commonality>2</commonality>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
				<li>
					<weight>6</weight>
					<only>RGBA(1,1,1,1)</only>
				</li>
      </options>
    </colorGenerator>
    </ThingDef>

